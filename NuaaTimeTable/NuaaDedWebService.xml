  <wsdl:portType name="WebServiceSoap">
    <wsdl:operation name="GetUsrInfoByUserID">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">根据用户id获取对应的用户的基本信息（用户类型、用户姓名、是否被禁用）</wsdl:documentation>
      <wsdl:input message="tns:GetUsrInfoByUserIDSoapIn" />
      <wsdl:output message="tns:GetUsrInfoByUserIDSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GetStuInfoByXh">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">根据给予的学号，返回该学号对应学生的基本学籍信息（年级、学院、班级、是否在校、是否有学籍）</wsdl:documentation>
      <wsdl:input message="tns:GetStuInfoByXhSoapIn" />
      <wsdl:output message="tns:GetStuInfoByXhSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GetTeacherInfoByXh">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">根据给予的教师工号，返回教师的基本信息（工号、姓名、所在单位）</wsdl:documentation>
      <wsdl:input message="tns:GetTeacherInfoByXhSoapIn" />
      <wsdl:output message="tns:GetTeacherInfoByXhSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GetXsmList">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">获取单位的基本信息（系所号、系所名、系所名简称、英文名、单位性质）</wsdl:documentation>
      <wsdl:input message="tns:GetXsmListSoapIn" />
      <wsdl:output message="tns:GetXsmListSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GetCurrentXlInfo">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">获取当天校历信息(日期、学年、学期、周次、备注&lt;寒暑&gt;)</wsdl:documentation>
      <wsdl:input message="tns:GetCurrentXlInfoSoapIn" />
      <wsdl:output message="tns:GetCurrentXlInfoSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GeXlInfoByDate">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">根据日期获取校历信息(日期、学年、学期、周次、备注&lt;寒暑&gt;)</wsdl:documentation>
      <wsdl:input message="tns:GeXlInfoByDateSoapIn" />
      <wsdl:output message="tns:GeXlInfoByDateSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GetBjKb">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">根据给予的班号和查询时间返回课表,某一节次（jc为单个数字）的课程名</wsdl:documentation>
      <wsdl:input message="tns:GetBjKbSoapIn" />
      <wsdl:output message="tns:GetBjKbSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GetBjKbAll">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">根据给予的班号的学期课表</wsdl:documentation>
      <wsdl:input message="tns:GetBjKbAllSoapIn" />
      <wsdl:output message="tns:GetBjKbAllSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GetCourseTableByXh">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">根据给予的学号的个人课表——本功能在测试中</wsdl:documentation>
      <wsdl:input message="tns:GetCourseTableByXhSoapIn" />
      <wsdl:output message="tns:GetCourseTableByXhSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="Verify">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">验证用户名和密码是否正确，如果用户名不存在或者密码错误均返回False，只有用户名和密码匹配才返回True</wsdl:documentation>
      <wsdl:input message="tns:VerifySoapIn" />
      <wsdl:output message="tns:VerifySoapOut" />
    </wsdl:operation>
  </wsdl:portType>
